

.main-wrap{
  letter-spacing: 0.03px;
  box-sizing: border-box;
  absolute left 200px top 67px bottom 0 right 0
  overflow: auto;
  box-sizing: border-box;
  padding: 0 42px;
  // 此为一行
  .row{
    position: relative;
    width: 100%;
  }
  // 面板公用类
  .panel {
    background: #fff;
    padding: 0 48px 20px;
    box-sizing: border-box;
    margin: 0 10px 20px;
    border-radius: 10px;
  }

  //页面标题
  .title{
    background: transparent;
    padding: 0;
    margin: 0 10px;
    height: 146px;
    vertical-align: middle;
    padding-top: 52px;
    box-sizing: border-box;
    position relative
    
    .label1{
      color: #2b2e33;
      font-size: 24px;
    }
      
    .label2{
      color: #959698;
    }
  }
  
}
@media screen and (max-width: 1440px){
  .main-wrap {
    padding: 0 10px;
  }
}