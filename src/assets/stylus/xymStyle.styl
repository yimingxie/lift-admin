// xie公用样式

// 告警颜色规范定义
$warningColor = #4BCC8F; // 预警
$violationColor = #8025FD; // 违规
$faultColor = #FFA90B; // 故障
$accidentColor = #FA4F43; // 事件
.warningColor{
  color: $warningColor !important; 
}
.violationColor{
  color: $violationColor !important;
}
.faultColor{
  color: $faultColor !important;
}
.accidentColor{
  color: $accidentColor !important;
}





// 列表
.lift-list{
  background #fff;
  margin 20px;
  border-radius 6px;
}
.lift-list-title{
  line-height 50px;
  padding-left 20px;
  font-size: 16px;
  color: #34414C;
  border-bottom 1px solid #D8DDDF;  
}
.ll-choose{
  padding 0 30px;
}
.ll-choose-top{
  padding 20px 0;
}
.llct-area{
  float left;
  width 160px;
  height 30px;
}
.llct-line{
  float left
  width 1px;
  height 14px;
  margin-top 10px;
  background #D8DDDF;
}
.llct-type{
  float left;
  margin-left 14px;
  margin-top 2px;
}
.llct-type em{
  float left;
  display inline-block;
  font-style normal;
  font-size 14px;
  line-height 28px;
  padding 0 10px;
  cursor pointer;
  border-radius 4px;
}
.llct-type span{
  float left;
  display inline-block;
  font-size 14px;
  line-height 22px;
  padding 0 10px;
  cursor pointer;
  color: #7E8A95;
  margin-right 5px;
  border-radius 4px;
  cursor pointer;
}
.llct-type span.on,.llct-type span:hover{
  background: #4272FF;
  color #fff;
}
.ll-choose-bottom{
  padding 20px 0;
  border-top 1px dashed  #D8DDDF;
}
.llcb-operate{
  float left
}
.llcb-btn{
  display inline-block;
  font-size 14px;
  color: #34414C;
  border: 1px solid #D8DDDF;
  border-radius: 4px;
  padding 0 16px;
  cursor pointer;
  line-height 32px;
}
.llcb-operate .llcb-btn{
  margin-right 5px;
}
.llcb-btn.info{
  background: #4272FF;
  border: 1px solid #4272FF;
  color #fff;
}





.ll-table{
  border-top 10px solid #F5F6F7;
  padding 20px;
}
.llt-thead{
  background: #F5F6F7;
}
.llt-th{
  position relative;
  box-sizing border-box;
  float left;
  font-size: 14px;
  color: #34414C;
  line-height: 22px;
  height 54px;
  padding 16px 20px;
  overflow hidden;
  text-overflow ellipsis;
  white-space nowrap;
}
.llt-th-sort{
  display inline-block;
  padding-right 16px;
  background: url('../../assets/images/xym/sort.png') no-repeat right center;
  cursor pointer;
}
.llt-th-sort.asc{
  background: url('../../assets/images/xym/sort-asc.png') no-repeat right center;
}
.llt-th-sort.desc{
  background: url('../../assets/images/xym/sort-desc.png') no-repeat right center;
}
.llt-tbody .llt-tr{
  position relative;
  border-bottom: 1px solid #E8E8E8;
}
.llt-td{
  position relative;
  box-sizing border-box;
  float left;
  font-size: 14px;
  color: #34414C;
  line-height: 22px;
  height 54px;
  padding 16px 20px;
  overflow hidden;
  text-overflow ellipsis;
  white-space nowrap;
}

.llt-td-device{
  display inline-block;
  cursor pointer;
}
.llt-td-a{
  color #4272FF;
  cursor pointer;
}
.llt-td-line{
  vertical-align middle;
  font-style normal;
  color: #D8DDDF;
  display inline-block;
  margin: 0 2px;
  font-size 12px;
  margin-top -4px;
}
.llt-td-type{
  display inline-block;
  margin-left 8px;
}
.llt-td .llt-td-type:first-child{
  margin-left 0;
}
.llt-td-type span{
  margin-left 4px;
}
.llt-td-type span.warning{
  color: #9ACF63;
}
.llt-td-type span.illegal{
  color: #FFC60B;
}
.llt-td-type span.fault{
  color: #FF980B;
}
.llt-td-type span.accident{
  color: #E9645D;
}
.llt-td-arrow{
  position: absolute;
  top: 18px;
  right 26px;
  width 20px;
  height 20px;
  background: url('../../assets/images/xym/down-arrow.png') no-repeat center center;
  cursor pointer
  transition: all 0.5s ease;
}
.llt-tr.on .llt-td-arrow{
  // background: url('../../assets/images/xym/up-arrow.png') no-repeat center center;
  transform: rotate(180deg)
}
.llt-drop{
  background-image: linear-gradient(180deg, rgba(232,233,236,0.70) 0%, #FBFBFB 4%, #FFFFFF 8%, #FFFFFF 50%, #FFFFFF 93%, #FBFBFB 97%, #E8E9EC 100%);
  overflow: auto;
  height 0;
  transition: all 0.5s ease
}
.llt-tr.on .llt-drop.heightAuto{
  height auto;
}
.llt-tr.on .llt-drop.heightFix{
  height 475px;
}
.llt-drop-wrap{
  box-sizing: border-box;
  padding 0 20px;
  margin-left 43%;
}
.llt-drop-box{
  position relative;
  border-bottom: 1px solid #E8E8E8;
  padding 20px 0;
}
.ldrop-detail-a{
  position absolute;
  top 40px;
  right 0;
  font-size: 14px;
  color: #4272FF;
  line-height: 22px;
  cursor pointer;
}
.ldrop-left{
  padding-right 70px;
}
.ldrop-title{
  font-size: 16px;
  color: #000;
  line-height: 22px;
}
.llt-drop-box.offline .ldrop-title{
  color: #C2C7CC;
}
.ldrop-detail{
  overflow hidden;
  text-overflow: ellipsis;
  white-space nowrap;
  margin-top 10px;
}
.ldd-status{
  display inline-block;
  font-size: 14px;
  color: #4272FF;
  line-height: 22px;
}
.llt-drop-box.offline .ldd-status{
  color: #C2C7CC;
}
.ldd-line{
  vertical-align: middle;
  font-size: 12px;
  display inline-block;
  color: #7E8A95;
  margin-top -4px;
  margin-left 6px;
  margin-right 6px;
}
.ldd-id{
  display inline-block;
  font-size: 14px;
  color: #7E8A95;
  line-height: 22px;
}
.llt-drop-box.offline .ldd-id{
  color: #C2C7CC;
}
.idd-project{
  display inline-block;
  font-size: 14px;
  color: #34414C;
  line-height: 22px;
  margin-left 18px;
}
.llt-drop-box.offline .idd-project{
  color: #C2C7CC;
}
.idd-project span{
  color: #7E8A95;
}
.llt-drop-box.offline .idd-project span{
  color: #C2C7CC;
}
