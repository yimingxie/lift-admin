// 检测
.det-mid{
  float: left;
  width 35%;
  margin-left 1.5%;
}
.det-mid-status{
  height 495px;
  background-image: linear-gradient(90deg, #4272FF 0%, #6159FF 100%);
  box-shadow: 0 -8px 20px -12px #1D41AB;
  border-radius: 8px;
}
.dms-title{
  position relative;
  border-bottom: 1px solid rgba(255,255,255,0.3)
  line-height 60px;
  padding: 0 20px;
}
.dms-title h4{
  font-size: 16px;
  color: #FFFFFF;
}
.dms-title-balance{
  position: absolute;
  top: 0;
  right 20px;
  font-size: 14px;
  color: rgba(255,255,255,0.65);
  cursor pointer
}
.dms-lift{
  position relative;
}
.dms-lift-pic{
  float left;
  width 180px;
  margin-top 10px;
  margin-left 24px;
}
.dms-lift-derection{
  position: absolute;
  top: 80px;
  left 50%;
  width 70px;
  margin-left -35px;
  text-align: center;
}
.dmsl-dt{
  position relative
  width 24px;
  height 24px;
  margin: 0 auto;
}
.dmsl-dt-up{
  background: url('../../assets/images/xym/det-up.png') no-repeat;
}
.dmsl-dt-up-on{
  position: absolute;
  top: -8px;
  left 0;
  width 24px;
  height 24px;
  background: url('../../assets/images/xym/det-up-on.png') no-repeat;
  display none;
}
.dmsl-dt-down{
  background: url('../../assets/images/xym/det-down.png') no-repeat;
  margin-top 10px;
}
.dmsl-dt-down-on{
  position: absolute;
  top: 0;
  left 0;
  width 24px;
  height 24px;
  background: url('../../assets/images/xym/det-down-on.png') no-repeat;
  display none;
}
.dmsl-dt-floor{
  color: #fff;
}
.dmsl-dt-floor h4{
  font-size: 14px;
}
.dmsl-dt-floor h4 span{
  font-size: 24px;
}
.dmsl-dt-floor p{
  font-size: 12px;
  color: rgba(255,255,255,0.65);
  margin-top 10px;
}
.dms-lift-data{
  width 110px;
  float right;
  margin-right: 20px;
}
.dmsl-data{
  color: #fff;
  margin-top 26px;
}
.dmsl-data h4{
  font-size: 14px;
}
.dmsl-data h4 span{
  font-size: 24px;
}
.dmsl-data p{
  font-size: 12px;
  color: rgba(255,255,255,0.65);
  margin-top 10px;
}
.dms-lift-info{
  margin: 0 20px;
  border-top: 1px dashed rgba(255,255,255,0.30);
}
.dms-lift-info .dmsl-data{
  box-sizing: border-box;
  padding-left 10px;
  float left;
  width 33%;
  margin-top 20px;  
}
.det-mid-log{
  height 267px;
  background: #FFFFFF;
  box-shadow: 0 8px 20px -12px rgba(66,114,255,0.30);
  border-radius: 8px;
  margin-top 20px;
}
.dm-go-detail{
  position: absolute;
  top: 0;
  right 20px;
  line-height 60px;
  font-weight: normal;
  font-size: 14px;
  color: #7E8A95;
  cursor pointer;
}
.det-mid-log .det-warn-title{
  border-bottom: 1px solid #D8DDDF;
}
.det-right .det-warn-title{
  border-bottom: 1px solid #D8DDDF;
}
.dm-log-content{
  padding-top 10px;
  height 190px;
  overflow: auto;
}
.dm-log-list{
  line-height 36px;
  font-size: 14px;
  color: #7E8A95;
  cursor pointer;
}
.dm-log-list:hover{
  background: rgba(66,114,255,0.10);
}
.dmll-td{
  box-sizing: border-box;
  float left;
  overflow hidden;
  text-overflow: ellipsis;
  white-space nowrap;
  padding: 0 10px;
}
.dm-log-list:hover .dmll-td{
  color: #4272FF !important;
}
.dm-log-list .dmll-td:nth-child(1){
  color: #34414C;
  padding-left 20px;
  width 40%;
}
.dm-log-list .dmll-td:nth-child(2){
  width 20%;
}
.dm-log-list .dmll-td:nth-child(3){
  width 40%;
  text-align: right;
  padding-right: 20px;
}
.det-right{
  float right;
  background: #FFFFFF;
  box-shadow: 0 8px 20px -12px rgba(66,114,255,0.30);
  border-radius: 8px;
  height 783px;
  width 40%;
}
.det-cTime-con{
  position: absolute;
  top: 16px;
  right 20px;
}
.dcc-btn-current{
  float left
  width 56px;
  line-height 24px;
  height 24px;
  text-align: center;
  background: rgba(66,114,255,0.3);
  color #fff;
  cursor pointer;
}
.dcc-btn-current.on{
  background: rgba(66,114,255,1);
}
.dcc-btn{
  float left;
  width 24px;
  height 24px;
  box-shadow: 0 2px 4px 0 rgba(66,114,255,0.30);
  border-radius: 4px;
  cursor pointer;
  margin: 0 4px;
  box-shadow: 0 2px 4px 0 rgba(66,114,255,0.30);
}
.dcc-btn-left{
  background: #4272FF url('../../assets/images/xym/dcc-left.png') no-repeat center center;
}
.dcc-btn-right{
  background: rgba(66,114,255,1) url('../../assets/images/xym/dcc-right.png') no-repeat center center;
}

