// 表格--start
.el-table
    color #fff!important
.el-table th, .el-table tr
    background-color #16161C!important
.tableBg.el-table th, .tableBg.el-table tr
    background-color #1B1B23!important
.el-table--enable-row-hover .el-table__body tr:hover>td
    background-color:rgba(102,102,127,0.23)!important
.el-table--border, .el-table--group
    border: none!important
.el-table td, .el-table th.is-leaf
    border-bottom: 1px solid rgba(102,102,127,0.23)!important;
.el-table--border td, .el-table--border th, .el-table__body-wrapper .el-table--border.is-scrolling-left~.el-table__fixed
    border-right none!important
.el-table::before
    height: 0!important
.el-table--border::after, .el-table--group::after
    width: 0!important
.el-table td, .el-table th
    padding 6px 0!important
.el-table thead
    color #66667F!important;
.el-table .descending .sort-caret.descending {  // 排序
    border-top-color: #0DBA7F!important;
}
.el-table .ascending .sort-caret.ascending { // 排序
    border-bottom-color: #0DBA7F!important;
}
.el-table .cell, .el-table th div, .el-table--border td:first-child .cell, .el-table--border th:first-child .cell
    padding-left: 0px!important;
.el-table .cell, .el-table th div
    padding-right: 2px!important;
.el-table, .el-table__expanded-cell
    background-color #1B1B23!important
.el-table .sort-caret
    border: 4px solid transparent!important
.el-table .sort-caret.ascending
    border-bottom-color: #c0c4cc!important;
.el-table .sort-caret.descending
    border-top-color: #c0c4cc!important;
.el-table .caret-wrapper
    height: 30px!important;
.el-table__column-filter-trigger
    line-height: 24px!important;
    margin-left: 10px!important;

.el-table__empty-text
    color #66667F!important;
// 表格--end



    

